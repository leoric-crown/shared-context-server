{
  "mcpServers": {
    "shared-context-server": {
      "type": "http",
      "url": "http://localhost:23432/mcp/",
      "headers": {
        "X-API-Key": "${API_KEY}"
      },
      "disabled": false,
      "autoApprove": [
        "authenticate_agent",
        "create_session",
        "add_message",
        "search_context",
        "set_memory",
        "get_memory",
        "get_messages",
        "refresh_token",
        "search_by_sender",
        "search_by_timerange",
        "get_session"
      ]
    },
    "octocode": {
      "command": "npx",
      "args": [
        "-y",
        "octocode-mcp"
      ],
      "disabled": false,
      "autoApprove": [
        "githubSearchCode",
        "githubGetFileContent",
        "githubViewRepoStructure",
        "githubSearchRepositories",
        "githubSearchCommits",
        "packageSearch"
      ]
    }
  }
}
